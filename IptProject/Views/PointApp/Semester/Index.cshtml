@model IEnumerable<PointDataAccess.Semester>

@{
    ViewBag.Title = "All Semester";
}

<h2>All Semester</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SemesterName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SemesterType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SemesterName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SemesterType)
        </td>
    </tr>
}

</table>
