@model IEnumerable<PointDataAccess.Student>

@{
    ViewBag.Title = "Students";
}

<h2>Students</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MobileNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RollNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CandidateID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BatchID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SPassword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProgrammeID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MobileNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RollNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CandidateID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BatchID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SPassword)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProgrammeID)
        </td>
    </tr>
}

</table>
